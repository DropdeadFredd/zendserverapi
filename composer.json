{
    "name": "zendserverapi/zendserverapi",
    "description": "Zend Server API Wrapper",
    "type": "library",
    "license": "MIT",
    "version": "0.0.1",
    "keywords": [
        "zend server",
        "api"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "homepage": "https://github.com/iwalz/zendserverapi",
    "authors": [
        {
            "name": "Ingo Walz",
            "email": "ingo.walz@googlemail.com",
            "homepage": "http://www.rubber-duckling.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "guzzle/guzzle": "3.0.2",
		"zendframework/zend-log": "2.0.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "ZendServerAPI": "src/",
            "ZendServerAPITest": "tests/"
        }
    }
}
